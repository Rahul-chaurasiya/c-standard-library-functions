

#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    double base, exponent, result;

    cout << "enter base :" << endl;
    cin >> base;

    cout << "enter exponent :" << endl;
    cin >> exponent;

    result = pow(base, exponent);

    cout << base << "^" << exponent << " = " << result;

    return 0;
}



output :
enter base :
4
enter exponent :
3
4^3 = 64
